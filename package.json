{
  "name": "safe-framework",
  "version": "0.0.7",
  "description": "Safe Framework",
  "repository": "https://github.com/jeffshaver/safe-framework",
  "main": "./lib",
  "scripts": {
    "test": "npm run build && npm run lint-all",
    "lint": "eslint `find src/ -type f -name '*.js*'`",
    "lint-examples": "eslint `find examples/js/ -type f -name '*.js*'`",
    "lint-all": "npm run lint && npm run lint-examples",
    "build": "babel ./src --out-dir ./lib",
    "build-examples": "npm run build && cp -R lib/* examples/node_modules/safe-framework/lib/",
    "prebuild": "rimraf lib",
    "prepublish": "npm run test"
  },
  "author": "Jeff Shaver <jeff.e.shaver@gmail.com> (http://jeffshaver.me)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "5.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "babelify": "^7.2.0",
    "eslint": "^2.1.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "^3.9.0",
    "gulp-shell": "0.5.2",
    "rimraf": "^2.4.3"
  },
  "dependencies": {
    "color": "^0.10.1",
    "flexboxgrid": "^6.3.0",
    "highcharts": "^4.2.3",
    "color": "^0.10.1"
  },
  "peerDependencies": {
    "material-ui": "^0.14.1",
    "radium": "^0.14.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-highcharts": "^7.0.0",
    "react-leaflet": "^0.10.0",
    "react-tap-event-plugin": "0.2.1"
  }
}
